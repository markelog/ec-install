{
  "name": "eclectica",
  "version": "0.6.0",
  "description": "Cool and eclectic version manager for any language",
  "scripts": {
    "postinstall": "EC_DEST=$(npm config get prefix)/bin EC_VERSION=0.6.0 ./scripts/install.sh"
  },
  "bin": {
    "ec": "./bin/ec",
    "ec-proxy": "./bin/ec-proxy"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markelog/eclectica.git"
  },
  "keywords": [
    "eclectic",
    "eclectica",
    "ruby",
    "golang",
    "go",
    "rust",
    "rustc",
    "node",
    "nodejs",
    "version",
    "manager",
    "switcher",
    "binary",
    "env",
    "environment"
  ],
  "files": ["scripts/install.sh", "bin"],
  "author": "Oleg Gaidarenko <markelog@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markelog/eclectica/issues"
  },
  "homepage": "https://github.com/markelog/eclectica#readme"
}
